<svg xmlns="http://www.w3.org/2000/svg" width="300" height="450" viewBox="0 0 300 450" fill="none">
  <!-- Background -->
  <rect width="300" height="450" fill="#1f2937"/>
  
  <!-- Film strip holes -->
  <g fill="#374151">
    <rect x="10" y="20" width="8" height="8" rx="2"/>
    <rect x="282" y="20" width="8" height="8" rx="2"/>
    <rect x="10" y="50" width="8" height="8" rx="2"/>
    <rect x="282" y="50" width="8" height="8" rx="2"/>
    <rect x="10" y="80" width="8" height="8" rx="2"/>
    <rect x="282" y="80" width="8" height="8" rx="2"/>
    <rect x="10" y="370" width="8" height="8" rx="2"/>
    <rect x="282" y="370" width="8" height="8" rx="2"/>
    <rect x="10" y="400" width="8" height="8" rx="2"/>
    <rect x="282" y="400" width="8" height="8" rx="2"/>
    <rect x="10" y="430" width="8" height="8" rx="2"/>
    <rect x="282" y="430" width="8" height="8" rx="2"/>
  </g>
  
  <!-- Main content area -->
  <rect x="40" y="60" width="220" height="330" fill="#374151" rx="8"/>
  
  <!-- Movie icon -->
  <g fill="#6b7280" transform="translate(150, 200)">
    <!-- Camera body -->
    <rect x="-30" y="-20" width="60" height="40" rx="4"/>
    <!-- Lens -->
    <circle cx="0" cy="0" r="15" fill="#4b5563"/>
    <circle cx="0" cy="0" r="8" fill="#374151"/>
    <!-- Film reel -->
    <circle cx="-20" cy="-10" r="6" fill="#6b7280"/>
    <circle cx="20" cy="-10" r="6" fill="#6b7280"/>
  </g>
  
  <!-- Text -->
  <text x="150" y="280" text-anchor="middle" fill="#9ca3af" font-family="Arial, sans-serif" font-size="14" font-weight="500">
    Movie Poster
  </text>
  <text x="150" y="300" text-anchor="middle" fill="#6b7280" font-family="Arial, sans-serif" font-size="12">
    Not Available
  </text>
  
  <!-- Border -->
  <rect x="1" y="1" width="298" height="448" stroke="#374151" stroke-width="2" fill="none" rx="4"/>
</svg>
